			    UPPER( COALESCE('Sales Report No Filter'[Material], "") )
			VAR SoldTo =
			    UPPER( COALESCE('Sales Report No Filter'[Sold to name1], "") )
			RETURN
			    SWITCH(
			        TRUE(),
			
			        -- Material
			        LEFT(Mat, 7) = "STARTUP", "STARTUP",
			        LEFT(Mat, 3) = "GTB", "GTB",
			        CONTAINSSTRING(Mat, "CHARGES"), "CHARGES",
			        CONTAINSSTRING(Mat, "EXTENDED") || CONTAINSSTRING(Mat, "EXTSERV"), "EXTWAR/SERV",
			        CONTAINSSTRING(Mat, "FRAIS")
			            || CONTAINSSTRING(Mat, "INSP")
			            || CONTAINSSTRING(Mat, "SUP")
			            || CONTAINSSTRING(Mat, "SUB-C")
			            || CONTAINSSTRING(Mat, "OPTION")
			            || CONTAINSSTRING(Mat, "STORAGE"), "FRAIS",
			
			        -- Sold-to
			        CONTAINSSTRING(SoldTo, "VIESSMANN"), "Product/Viessmann",
			
			        "Product"
