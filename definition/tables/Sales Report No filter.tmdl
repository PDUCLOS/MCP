	column Statut_SOITEM =
			
			VAR ValeurCourante = 'Sales Report No filter'[SOITEM]
			VAR DateCourante = 'Sales Report No filter'[Outbound Delivery]
			
			VAR MinDateSOITEM =
			    CALCULATE(
			        MIN('Sales Report No filter'[Outbound Delivery]),
			        FILTER(
			            ALL('Sales Report No filter'),
			            'Sales Report No filter'[SOITEM] = ValeurCourante
			                && NOT(ISBLANK('Sales Report No filter'[Outbound Delivery]))
			        )
			    )
			
			VAR NbLignesSOITEM =
			    CALCULATE(
			        COUNTROWS('Sales Report No filter'),
			        FILTER(
			            ALL('Sales Report No filter'),
			            'Sales Report No filter'[SOITEM] = ValeurCourante
			        )
			    )
			
			RETURN
			    IF(
			        ISBLANK(ValeurCourante),
			        BLANK(),
			        IF(
			            NbLignesSOITEM = 1,
			            "Unique",
			            IF(
			                NOT(ISBLANK(DateCourante)) && DateCourante = MinDateSOITEM,
			                "Doublon inclus",
			                "Doublon exclu"
			            )
			        )
