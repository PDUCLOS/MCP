			VAR ZA = 'Sales Report'[ZA:Factory Confirmed date]
			VAR ZB = 'Sales Report'[ZB:Last Availability Date Confirmed]
			VAR Vendor = 'Sales Report'[Supplier name]
			RETURN
			    IF(
			        ISBLANK(ZD),
			        "Not Produced",
			        IF(
			            Vendor IN {"CARRIER MONTILLA, S.A.", "CARRIER CULOZ SA"},
			            IF(
