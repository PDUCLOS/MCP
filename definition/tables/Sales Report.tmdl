	column 'Number days Created ZA (ouvrés, inclut départ)' =
			
			VAR CreationDate = DATEVALUE ( 'Sales Report'[SO Creation Date] )
			VAR FactoryDate  = DATEVALUE ( 'Sales Report'[ZA:Creation Date] )
			VAR MinD = MIN ( CreationDate, FactoryDate )
			VAR MaxD = MAX ( CreationDate, FactoryDate )
			VAR Sign = IF ( FactoryDate >= CreationDate, 1, -1 )
			RETURN
			IF (
			    ISBLANK ( FactoryDate ) || ISBLANK ( CreationDate ),
			    BLANK (),
			    Sign *
			        CALCULATE (
			            COUNTROWS ( 'Calendar' ),
			            FILTER (
			                'Calendar',
			                'Calendar'[Date] >= MinD
			                    && 'Calendar'[Date] <= MaxD
			                    && 'Calendar'[IsWorkingDay] = TRUE ()
			            )
			        )
			)
		formatString: 0
		lineageTag: 84f7f8fe-5ba0-4f49-a71e-378cfa361332
		summarizeBy: sum

