	column 'Delivery status Controle ZA' = ```
			
			SWITCH(
			    TRUE(),
			    
			    // Cas 1 : CARRIER CULOZ SA ou CARRIER MONTILLA, S.A.
			    'Sales Report'[Supplier name] IN { "CARRIER CULOZ SA", "CARRIER MONTILLA, S.A." },
			    IF (
			        'Sales Report'[ZC:Previous availability date] <= 'Sales Report'[ZA:Factory Confirmed date],
			        IF (
			            ISBLANK('Sales Report'[ZB:Last Availability Date Confirmed]),
			            "NO ZB",
			            IF (
			                'Sales Report'[ZB:Last Availability Date Confirmed] <= 'Sales Report'[ZA:Factory Confirmed date],
			                "OK",
			                "LATE"
			            )
			        ),
			        IF (
			            'Sales Report'[ZB:Last Availability Date Confirmed] >= 'Sales Report'[ZA:Factory Confirmed date],
			            "OK",
			            "2 MODIFICATION"
			        )
			    ),
			
			    // Cas 2 : CARRIER MONTLUEL SCS
			    'Sales Report'[Supplier name] = "CARRIER MONTLUEL SCS",
			    IF (
			        'Sales Report'[ZC:Previous availability date] <> BLANK(),
			        "ONE MODIFICATION",
			        IF (
			            'Sales Report'[ZB:Last Availability Date Confirmed] > 'Sales Report'[ZA:Factory Confirmed date],
			            "LATE",
			            "OK"
			        )
			    ),
			
			    // Cas par défaut
			    BLANK()
			)
			
			
			```
